<h1>Task List</h1>
<p><%= @msg %></p>
<table class="table table-dark table-striped">
  <tr>
    <th>#</th><th>title</th><th></th><th></th>
  </tr>
  <% @data.each do |obj| %>
  <tr>
    <td><%= obj.id %></td>
    <td><%= link_to(obj.title,"/#{obj.id}",{class:"link-light"})%></td>
    <td><%= link_to("Edit","/edit/#{obj.id}",{class:"link-light"})%></td>
    <td><%= link_to("Delete","/#{obj.id}",class: "link-light" ,method: :delete, data: {confirm: "削除しますか？"})%></td>
  </tr>
  <% end %>
</table>
